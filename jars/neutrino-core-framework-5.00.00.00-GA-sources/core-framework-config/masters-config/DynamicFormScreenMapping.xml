<com.nucleus.makerchecker.GridConfiguration>
<entityClass>com.nucleus.core.formsConfiguration.DynamicFormScreenMapping</entityClass>
<jspName>masterDatatable</jspName>
<processingType>true</processingType>
<recordUrl>/app/DynamicFormScreenMapping/view/</recordUrl>
<entityNameMRKey>DynamicFormScreenIdMapping</entityNameMRKey>
<defaultSortable>id</defaultSortable>
<sortDirection>ASC</sortDirection>
<masterEntityConfiguration>
	<autoApproval>false</autoApproval>
	<entityUpdateInfoList>
	</entityUpdateInfoList>
</masterEntityConfiguration>

<springBeanName>masterGridService</springBeanName>

<key>DynamicFormScreenMapping</key>
	<initializingPath></initializingPath>
	<columnConfigurationList>
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.uuid</titleKey>
				<width>125</width>
				<sortable>false</sortable>
				<searchable>false</searchable>
				<hidden>true</hidden>
				<dataField>viewProperties.uuid</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.id</titleKey>
				<width>125</width>
				<sortable>false</sortable>
				<searchable>false</searchable>
				<hidden>false</hidden>
				<dataField>id</dataField>
				<columnType>link</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
			
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.task.id</titleKey>
				<width>125</width>
				<sortable>false</sortable>
				<searchable>false</searchable>
				<hidden>true</hidden>
				<dataField>viewProperties.taskId</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>label.screenName</titleKey>
				<width>100</width>
				<sortable>true</sortable>
				<searchable>true</searchable>
				<hidden>false</hidden>
				<dataField>screenId.screenName</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>label.sourceProduct</titleKey>
				<width>100</width>
				<sortable>true</sortable>
				<searchable>true</searchable>
				<hidden>false</hidden>
				<dataField>sourceProduct.name</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		
		
		
		<com.nucleus.makerchecker.ColumnConfiguration>
					<titleKey>key.created.by</titleKey>
				<width>125</width>
				<sortable>true</sortable>
				<searchable>true</searchable>
				<hidden>false</hidden>
				<dataField>viewProperties.createdBy</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
			
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.reviewed.by</titleKey>
				<width>125</width>
				<sortable>false</sortable>
				<searchable>false</searchable>
				<hidden>false</hidden>
				<dataField>viewProperties.reviewedBy</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.status</titleKey>
				<width>145</width>
				<sortable>true</sortable>
				<searchable>true</searchable>
				<hidden>false</hidden>
				<dataField>viewProperties.approvalStatus</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		<com.nucleus.makerchecker.ColumnConfiguration>
			<titleKey>key.activeStatus</titleKey>
			<width>125</width>
			<sortable>true</sortable>
			<searchable>false</searchable>
			<hidden>false</hidden>
			<dataField>activeFlag</dataField>
			<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>


		<com.nucleus.makerchecker.ColumnConfiguration>
			<titleKey>key.created.on</titleKey>
			<width>125</width>
			<sortable>true</sortable>
			<searchable>false</searchable>
			<hidden>false</hidden>
			<dataField>entityLifeCycleData.creationTimeStamp</dataField>
			<columnType>dateTime</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>
		<com.nucleus.makerchecker.ColumnConfiguration>
			<titleKey>key.updated.on</titleKey>
			<width>125</width>
			<sortable>true</sortable>
			<searchable>false</searchable>
			<hidden>false</hidden>
			<dataField>entityLifeCycleData.lastUpdatedTimeStamp</dataField>
			<columnType>dateTime</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>


		<com.nucleus.makerchecker.ColumnConfiguration>
				<titleKey>key.actions</titleKey>
				<width>125</width>
				<sortable>false</sortable>
				<searchable>false</searchable>
				<hidden>false</hidden>
				<dataField>viewProperties.actions</dataField>
				<columnType>string</columnType>
		</com.nucleus.makerchecker.ColumnConfiguration>	
	</columnConfigurationList>
	<actionConfigurationList>
			<com.nucleus.makerchecker.ActionConfiguration>
				<action>Edit</action>
				<imagePath>/images/Edit.png</imagePath>
				<actionUrl>/app/DynamicFormScreenMapping/edit/</actionUrl>
				<text>false</text>
			</com.nucleus.makerchecker.ActionConfiguration>
			<com.nucleus.makerchecker.ActionConfiguration>
				<action>Delete</action>
				<imagePath>/images/Delete.png</imagePath>
				<actionUrl>/app/master/DynamicFormScreenMapping/DynamicFormScreenMapping/delete/</actionUrl>
				<text>false</text>
				<applicableForApprovedRecord>true</applicableForApprovedRecord>
			</com.nucleus.makerchecker.ActionConfiguration>
			<com.nucleus.makerchecker.ActionConfiguration>
				<action>SendForApproval</action>
				<imagePath>/images/Request_Approval.png</imagePath>
				<actionUrl>/app/master/DynamicFormScreenMapping/DynamicFormScreenMapping/approval/</actionUrl>
				<text>false</text>
			</com.nucleus.makerchecker.ActionConfiguration>
	</actionConfigurationList>
</com.nucleus.makerchecker.GridConfiguration>

